GridBackground {
    --grid-background-color: rgb(40, 40, 40);
    --line-color: rgba(193, 196, 192, 0.1);
    --thick-line-color: rgba(193, 196, 192, 0.1);
    --spacing: 15;
}


.graphView {
    flex-grow: 1;
}

.graphView .edge {
    --edge-width: 3;
}

.graphView .edge:checked {
    --edge-width: 3;
}
